document.write ('<A HREF="http://oas.rsc.org/5c/www.rsc.org/periodic-table/element/1/hydrogen/L34/1814390827/x02/RSC/Nanalysis_2017/Nanalysis_125x125_April.gif/652f7947656c6a436f557341434b712f?x" target="_top"><IMG SRC="http://cdn.oas-eu1.adnxs.com/0/RSC/Nanalysis_2017/Nanalysis_125x125_April.gif"  WIDTH=125 HEIGHT=125 ALT="Advert: Continuous-flow synthesis with integrated benchtop NMR"  BORDER="0"></A>');
